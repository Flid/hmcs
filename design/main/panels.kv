#:import GamePanel game.gui.panel.GamePanel

<GamePanel>:
    canvas:
        Color:
            rgb: 0.5, 0.5, 0.5
        Rectangle:
            pos: self.pos
            size: self.size

    Button:
        size_hint: 0.9, 0.1
        pos_hint: {'x': 0.05, 'top': 0.95}
        text: 'Main Menu'
        on_press:
            app.switch_screen('menu_main')